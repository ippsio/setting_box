{
  "title": "[80:Hotkeys]",
  "rules":
  [
    {
      "description": "[81:right_option + 's' then Slack]",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "s", "modifiers": { "mandatory": [ "right_option" ] } },
          "to": [ { "shell_command": "open -a 'Slack'" } ]
        }
      ]
    },
    {
      "description": "[82:right_option + 't' then Terminal]",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "t", "modifiers": { "mandatory": [ "right_option" ] } },
          "to": [ { "shell_command": "open -a '/System/Applications/Utilities/Terminal.app'" } ]
        }
      ]
    },
    {
      "description": "[83:right_option + 'm' then Message]",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "m", "modifiers": { "mandatory": [ "right_option" ] } },
          "to": [ { "shell_command": "open -a '/System/Applications/Messages.app/Contents/MacOS/Messages'" } ]
        }
      ]
    },
    {
      "description": "[84:right_option + 'c' then Google Chrome]",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "c", "modifiers": { "mandatory": [ "right_option" ] } },
          "to": [ { "shell_command": "open -a 'Google Chrome'" } ]
        }
      ]
    },
    {
      "description": "[85:right_option + 'e' then Microsoft Edge]",
      "manipulators": [
        {
          "type": "basic",
          "from": { "key_code": "e", "modifiers": { "mandatory": [ "right_option" ] } },
          "to": [ { "shell_command": "open -a '/Applications/Microsoft Edge.app'" } ]
        }
      ]
    }
  ]
}

