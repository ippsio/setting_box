{
  "title": "[10:COMMAND] behaviors",
  "rules": [
    {
      "description": "[10:COMMAND]: + [hjkl/ ae/ d]",
      "manipulators": [
        {
          "from": {"key_code": "h", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "left_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "j", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "down_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "k", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "up_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "l", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "right_arrow"}],
          "type": "basic"
        },{
          "from": {"key_code": "a","modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "a", "modifiers":["control"]}],
          "type": "basic"
        },{
          "from": {"key_code": "e","modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "e", "modifiers":["control"]}],
          "type": "basic"
        },{
          "from": {"key_code": "d","modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "to": [{"key_code": "d", "modifiers":["control"]}],
          "type": "basic"
        }
      ]
    },

    {
      "description": "[10:COMMAND]: + [w/i,o/t/p/s/n/,/] on [terminal] deactivate default key-shortcuts.",
      "manipulators": [
        {
          "from": {"key_code": "w", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "i", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "o", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "t", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "p", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "s", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "n", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        },
        {
          "from": {"key_code": "comma", "modifiers": {"mandatory": ["left_command"], "optional": ["any"]}},
          "type": "basic",
          "conditions": [ { "type": "frontmost_application_if", "bundle_identifiers": [ "^com\\.apple\\.Terminal" ] } ]
        }
      ]
    }
  ]
}

